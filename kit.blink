option ClientOutput = "src/kit/client/net.luau"
option ServerOutput = "src/kit/server/net.luau"
option Typescript = true
option PromiseLibrary = "ReplicatedStorage.WTHShared.include.Promise"
option Casing = Camel

enum DamageKind = {
    Normal,
    Heavy,
    Super,
    Lethal
}

event damageSelfVariable {
    from: Client,
    type: Reliable,
    call: ManyAsync,
    data: u32
}

event damageSelf {
    from: Client,
    type: Reliable,
    call: ManyAsync,
    data: DamageKind
}
