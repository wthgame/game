{
    "name": "Welcome to Hell Kit",
    "emitLegacyScripts": false,
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "WTHKitServer": {
                "$path": "out/kit/server",
                "WTHServer": {
                    "$path": "out/shared/server"
                }
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Mechanics": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "Core": {
                    "$path": "mechanics/core"
                },
                "Template": {
                    "$path": "mechanics/Template.luau"
                },
                "TemplateNoComment": {
                    "$path": "mechanics/TemplateNoComment.luau"
                }
            },
            "MechanicUtils": {
                "$path": "mechanics/utils"
            },
            "WTHKitShared": {
                "$path": "out/kit/shared",
                "WTHShared": {
                    "$path": "out/shared/shared",
                    "include": {
                        "$path": "include",
                        "node_modules": {
                            "$className": "Folder",
                            "@rbxts": {
                                "$path": "node_modules/@rbxts"
                            },
                            "@flamework": {
                                "$path": "node_modules/@flamework"
                            }
                        }
                    }
                }
            },
            "WTHKitClient": {
                "$path": "out/kit/client",
                "WTHClient": {
                    "$path": "out/shared/client"
                }
            }
        }
    }
}
