{
    "name": "Welcome to Hell Kit",
    "emitLegacyScripts": false,
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "WTHServer": {
                "$path": "out/kit/server",
                "core": {
                    "$path": "out/core/server"
                }
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Mechanics": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "Core": {
                    "$path": "mechanics/core"
                },
                "Template": {
                    "$path": "mechanics/Template.luau"
                },
                "TemplateNoComment": {
                    "$path": "mechanics/TemplateNoComment.luau"
                }
            },
            "MechanicUtils": {
                "$path": "mechanics/utils"
            },
            "WTHShared": {
                "$path": "out/kit/shared",
                "core": {
                    "$path": "out/core/shared"
                },
                "include": {
                    "$path": "include",
                    "node_modules": {
                        "$className": "Folder",
                        "@rbxts": {
                            "$path": "node_modules/@rbxts"
                        },
                        "@flamework": {
                            "$path": "node_modules/@flamework"
                        }
                    }
                }
            },
            "WTHClient": {
                "$path": "out/kit/client",
                "core": {
                    "$path": "out/core/client"
                }
            }
        }
    }
}
