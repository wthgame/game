{
    "name": "Welcome to Hell Kit",
    "emitLegacyScripts": false,
    "servePlaceIds": [
        113552277431105
    ],
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "WTHServerCore": {
                "$path": "out/core/server"
            },
            "WTHServerKit": {
                "$path": "out/kit/server"
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Mechanics": {
                "$className": "Folder",
                "$ignoreUnknownInstances": true,
                "Core": {
                    "$path": "mechanics/core"
                },
                "Template": {
                    "$path": "mechanics/Template.luau"
                },
                "TemplateNoComment": {
                    "$path": "mechanics/TemplateNoComment.luau"
                }
            },
            "MechanicUtils": {
                "$path": "mechanics/utils"
            },
            "WTHSharedCore": {
                "$path": "out/core/shared",
                "include": {
                    "$path": "include",
                    "node_modules": {
                        "$className": "Folder",
                        "@rbxts": {
                            "$path": "node_modules/@rbxts"
                        },
                        "@flamework": {
                            "$path": "node_modules/@flamework"
                        }
                    }
                }
            },
            "WTHSharedKit": {
                "$path": "out/kit/shared"
            },
            "WTHClientCore": {
                "$path": "out/core/client"
            },
            "WTHClientKit": {
                "$path": "out/kit/client"
            }
        }
    }
}
